using System;

namespace Test
{
    class Problem2
    {
        public static void Main(string[] args)
        {
            var s = Console.ReadLine();
            if (s != null)
            {
                s = s.Trim();
                s = s.TrimStart('0');
                if (s=="") Console.WriteLine("1");
                else Console.WriteLine(s.Length);
            }
        }
    }
}
